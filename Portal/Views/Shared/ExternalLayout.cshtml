@model Portal.Models.GeneralModel

@{ String URL = "http://" + Model.MenuViewModel.URL;}
<!DOCTYPE html>
<html>
<head>
    <title>Portal Dashboard</title>

    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <link rel="shortcut icon" type="image/ico" href="~/Content/Images/favicon.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    @Styles.Render( URL+ "/Content/Css/CustomCss.css")
    <!-- BEGIN GLOBAL MANDATORY STYLES -->

    @Styles.Render( URL+ "/assets/global/plugins/font-awesome/css/fontapis.css")
    @Styles.Render( URL+ "/assets/global/plugins/font-awesome/css/font-awesome.min.css")
    @Styles.Render( URL+ "/assets/global/plugins/simple-line-icons/simple-line-icons.min.css")
    @Styles.Render( URL+ "/assets/global/plugins/bootstrap/css/bootstrap.min.css")
    @Styles.Render( URL+ "/assets/global/plugins/uniform/css/uniform.default.css")

    <!-- END GLOBAL MANDATORY STYLES -->



    <!-- BEGIN PAGE LEVEL PLUGIN STYLES -->
    @Styles.Render(URL + "/assets/global/plugins/morris/morris.css")
    <!-- END PAGE LEVEL PLUGIN STYLES -->

    <!-- BEGIN PAGE STYLES -->
    @Styles.Render(URL + "/assets/admin/pages/css/tasks.css")
    @Styles.Render(URL + "/assets/global/plugins/fullcalendar/fullcalendar.min.css")

    <!-- END PAGE STYLES -->

    <!-- BEGIN THEME STYLES -->
    @Styles.Render(URL + "/assets/global/css/components-rounded.css")
    @Styles.Render(URL + "/assets/global/css/plugins.css")
    @Styles.Render(URL + "/assets/admin/layout3/css/layout.css")
    @Styles.Render(URL + "/assets/admin/layout3/css/themes/default.css")
    @Styles.Render(URL + "/assets/admin/layout3/css/custom.css")


    @Scripts.Render(URL + "/assets/global/plugins/jquery.min.js")
    @Scripts.Render(URL + "/Content/Javascript/Json.js")
    @Scripts.Render(URL + "/assets/global/plugins/jquery-ui/jquery-ui-1.10.3.custom.min.js")
    @Scripts.Render(URL + "/assets/global/plugins/bootstrap/js/bootstrap.js")
    @Scripts.Render(URL + "/assets/global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.js")
    @Scripts.Render(URL + "/assets/global/plugins/jquery-inputmask/jquery.inputmask.bundle.min.js")
    @Scripts.Render(URL + "/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js")
    @Scripts.Render(URL + "/assets/global/plugins/jquery.blockui.min.js")  
    @Scripts.Render(URL + "/assets/global/plugins/jquery.cokie.min.js")          
    @Scripts.Render(URL + "/assets/global/plugins/uniform/jquery.uniform.js")      
    
    <!-- END CORE PLUGINS -->

    <!-- BEGIN PAGE LEVEL PLUGINS -->

    @Scripts.Render(URL + "/assets/global/plugins/morris/raphael-min.js")
    @Scripts.Render(URL +"/assets/global/plugins/jquery.sparkline.min.js")
    @Scripts.Render(URL +"/assets/global/plugins/moment.min.js")
    @*@Scripts.Render(URL +"/assets/global/plugins/fullcalendar/fullcalendar.js")*@

    <!-- END PAGE LEVEL PLUGINS -->

    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    @Scripts.Render(URL +"/assets/global/scripts/metronic.js")
    @Scripts.Render(URL +"/assets/admin/layout3/scripts/layout.js")
    @*   @Scripts.Render("~/assets/admin/layout3/scripts/demo.js")*@
    @*   @Scripts.Render("~/assets/admin/pages/scripts/index3.js")*@
    @Scripts.Render(URL +"/assets/admin/pages/scripts/tasks.js")
    @Scripts.Render(URL +"/Content/Javascript/Dashboard.js")

      <!-- END PAGE LEVEL SCRIPTS -->

    <!-- END THEME STYLES -->
</head>
<body>

    <div class="page-content" >

        <div class="container">
            @RenderSection("MyOtherJavascript", required: false)
            @RenderBody()
        </div>
    </div>
    

</body>
</html>
